        jumpInput.Text = tostring(jumpPowerValue)
    end
end)

-- Jump Boost Logic
local function applyJumpBoost(character)
    local hrp = character:WaitForChild("HumanoidRootPart")
    local humanoid = character:WaitForChild("Humanoid")
    local canBoost = true

    humanoid.StateChanged:Connect(function(_, newState)
        if jumpBoostEnabled and newState == Enum.HumanoidStateType.Jumping and canBoost then
            canBoost = false
            local bodyVel = Instance.new("BodyVelocity")
            bodyVel.MaxForce = Vector3.new(0, math.huge, 0)
            bodyVel.Velocity = Vector3.new(0, jumpPowerValue, 0)
            bodyVel.Parent = hrp
            task.delay(0.15, function()
                if bodyVel and bodyVel.Parent then
                    bodyVel:Destroy()
                end
            end)
            task.delay(0.7, function()
                canBoost = true
            end)
        end
    end)
end

if LocalPlayer.Character then applyJumpBoost(LocalPlayer.Character) end
LocalPlayer.CharacterAdded:Connect(applyJumpBoost)
-- Timer ESP
local timerOn = false
local tBtn = createButton("Lock Timer ESP",100,mainSec)
tBtn.MouseButton1Click:Connect(function()
timerOn = not timerOn
tBtn.Text = timerOn and "âœ“ Timer ESP ON" or "Lock Timer ESP"
for _, v in ipairs(Workspace:GetDescendants()) do
if v:IsA("BillboardGui") and v.Name == "TimerESP" then v:Destroy() end
end
if not timerOn then return end
local plots = Workspace:FindFirstChild("Plots")
if plots then
for _, plot in ipairs(plots:GetChildren()) do
if plot:IsA("Model") then
local lbl
for _, gui in ipairs(plot:GetDescendants()) do
if gui:IsA("BillboardGui") and gui:FindFirstChild("RemainingTime") then
lbl = gui.RemainingTime; break
end
end
if lbl then
local cf, size = plot:GetBoundingBox()
local p = Instance.new("Part",Workspace)
p.Size=Vector3.new(1,1,1); p.Anchored=true; p.Transparency=1; p.CanCollide=false
p.CFrame=CFrame.new(cf.Position + Vector3.new(0,size.Y/2+4,0))
local esp = Instance.new("BillboardGui",Workspace)
esp.Name="TimerESP"; esp.Adornee=p; esp.Size=UDim2.new(0,120,0,25); esp.AlwaysOnTop=true
local txt = Instance.new("TextLabel",esp)
txt.Size=UDim2.new(1,0,1,0); txt.BackgroundTransparency=1
txt.TextScaled=true; txt.Font=Enum.Font.SourceSansBold; txt.TextStrokeTransparency=0.3
local lastText, tickCount = "",0
RunService.RenderStepped:Connect(function()
if not timerOn or not esp.Parent then return end
local cur=lbl.Text or ""
if cur==lastText then tickCount+=1 else lastText, tickCount=cur,0 end
if cur~="" and cur~="00:00:00" and tickCount<120 then
txt.Text, txt.TextColor3 = cur, Color3.fromRGB(255,255,0)
elseif cur=="00:00:00" then
txt.Text, txt.TextColor3 = "EXPIRED", Color3.fromRGB(255,0,0)
else
txt.Text, txt.TextColor3 = "0", Color3.fromRGB(180,180,180)
end
end)
end
end
end
end
end)
