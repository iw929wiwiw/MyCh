-- Teleport & Fall Buttons
do
local function mkBtn(txt,y)
local b = Instance.new("TextButton", tpFrame)
b.Size = UDim2.new(1,0,0,36); b.Position = UDim2.new(0,0,0,y)
b.Text             = txt
b.Font             = Enum.Font.GothamBold
b.TextSize         = 15
b.BackgroundColor3 = Color3.fromRGB(50,50,50)
Instance.new("UICorner", b).CornerRadius = UDim.new(0,8)
return b
end
local tpBtn   = mkBtn("Teleport Up (Q)", 0)
local fallBtn = mkBtn("Fall Down (C)", 46)
tpBtn.MouseButton1Click:Connect(function()
local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
if hrp then
if not findSkyPart() and storedPart then storedPart:Clone().Parent = Workspace end
hrp.CFrame = hrp.CFrame + Vector3.new(0,450,0)
end
end)
fallBtn.MouseButton1Click:Connect(function()
local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
if hrp then
local part = findSkyPart()
if part then storedPart = part:Clon();
part:Destroy() end  
hrp.CFrame = hrp.CFrame - Vector3.new(0,6,0)  
end  
end)  
UserInputService.InputBegan:Connect(function(input, gp)  
if not gp then  
if input.KeyCode == Enum.KeyCode.Q then tpBtn:Activate()  
elseif input.KeyCode == Enum.KeyCode.C then fallBtn:Activate() end  
end  
end)  
end 
