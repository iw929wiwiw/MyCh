--===[ DUAL-KEY SYSTEM (MEMORY-BASED) ]===--
local WEEKLY_KEY    = "Korez_KerGenHub-7777"
local PERM_KEY      = "D7me"
local PERM_USER_ID  = 8425478707
local KEY_DURATION  = 7 * 24 * 60 * 60  -- 1 week
local DATA_FILE     = "korez_keydata.json"
-- Load saved key (if any)
local function loadKeyData()
if isfile(DATA_FILE) then
local ok, data = pcall(function()
return HttpService:JSONDecode(readfile(DATA_FILE))
end)
if ok and type(data)=="table" then
getgenv().SavedKey = data.key
getgenv().KeyTime  = data.time
end
end
end
-- Save key and timestamp
local function saveKeyData(k)
local data = {
key  = k,
time = tick()
}
writefile(DATA_FILE, HttpService:JSONEncode(data))
getgenv().SavedKey = k
getgenv().KeyTime  = tick()
end
-- Initialize
getgenv().SavedKey = nil
getgenv().KeyTime  = 0
loadKeyData()
-- Check validity
local function isKeyValid()
-- Permanent unlock for special user
if LocalPlayer.UserId == PERM_USER_ID and getgenv().SavedKey == PERM_KEY then
return true
end
-- Weekly key check
return getgenv().SavedKey == WEEKLY_KEY and (tick() - getgenv().KeyTime) < KEY_DURATION
end
--===[ FIND SKY PART FOR FALL LOGIC ]===--
local storedPart
local function findSkyPart()
for _, obj in pairs(Workspace:GetDescendants()) do
if obj:IsA("BasePart") then
local s, p = obj.Size, obj.Position
if math.abs(s.X - 55.35) < 1 and math.abs(s.Y - 466.29) < 1 and math.abs(s.Z - 495.07) < 1 and
math.abs(p.X + 347.07) < 1 and math.abs(p.Y - 142.23) < 1 and math.abs(p.Z - 55.01) < 1 then
return obj
end
end
end
return nil
end
